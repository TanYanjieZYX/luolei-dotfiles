<?xml version="1.0" encoding="utf-8"?>
<obprofiles xmlns="http://www.logitech.com/Cassandra/2013.1/OnboardProfile">
  <profile name="">
    <device powermode="1" buttoncount="11" model="Logitech.Gaming.Mouse.G602" reportrate="500" anglesnapping="0">
      <dpitable shift="500" currentindex="1" defaultindex="2">
        <dpi y="500" x="500"/>
        <dpi y="1000" x="1000"/>
        <dpi y="1500" x="1500"/>
        <dpi y="2000" x="2000"/>
        <dpi y="2500" x="2500"/>
      </dpitable>
    </device>
    <buttons>
      <button number="1" name="左键单击">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="leftclick"/>
        </mousefunction>
      </button>
      <button number="2" name="右键单击">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="rightclick"/>
        </mousefunction>
      </button>
      <button number="3" name="单击中键">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="middleclick"/>
        </mousefunction>
      </button>
      <button number="4" name="左 Cmd + ]">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="RBRACKET"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="5" name="左 Cmd + [">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="LBRACKET"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="6" name="左 Cmd + C">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="C"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="7" name="提高 DPI">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="dpiup"/>
        </mousefunction>
      </button>
      <button number="8" name="降低 DPI">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="dpidown"/>
        </mousefunction>
      </button>
      <button number="9" name="主页">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="HOME"/>
        </keystroke>
      </button>
      <button number="10" name="Ctrl + 向左">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="LEFT"/>
          <modifier value="LCTRL"/>
        </keystroke>
      </button>
      <button number="11" name="Ctrl + 向右">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="RIGHT"/>
          <modifier value="LCTRL"/>
        </keystroke>
      </button>
    </buttons>
  </profile>
</obprofiles>
